<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="RippleView">
        <attr name="rippleColor" format="color" />
        <attr name="alphaFactor" format="float" />
        <attr name="hover" format="boolean" />
    </declare-styleable>
    <declare-styleable name="ProcessButton">
        <attr name="progressText" format="string" />
        <attr name="completeText" format="string" />
        <attr name="colorProgress" format="color" />
        <attr name="colorComplete" format="color" />
    </declare-styleable>
    <declare-styleable name="FlatButton">
        <attr name="colorPressed" format="color" />
        <attr name="colorNormal" format="color" />
    </declare-styleable>
    <declare-styleable name="CircleImageView">
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
    </declare-styleable>

</resources>